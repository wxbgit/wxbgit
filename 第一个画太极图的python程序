from turtle import *
pensize(5)
penup()
goto(0,-300)
pendown()
begin_fill()
circle(300,180)
circle(150,180)
circle(-150,180)
end_fill()
circle(-300,180)
color('white')
penup()
goto(0,100)
pendown()
begin_fill()
circle(50)
end_fill()
color('black')
penup()
goto(0,-100)
pendown()
begin_fill()
circle(-50)
end_fill()
hideturtle()
done()


